{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/docs",
      "methods": ["GET"],
      "dest": "/src/index.ts"
    },
    {
      "src": "/api/docs/(.*)",
      "methods": ["GET"],
      "dest": "/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ]
}
