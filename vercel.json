{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/docs(?:/.*)?",
      "dest": "/src/index.ts"
    },
    {
      "src": "/swagger-ui.css",
      "dest": "src/index.js"
    },
    {
      "src": "/swagger-ui-bundle.js",
      "dest": "src/index.js"
    },
    {
      "src": "/swagger-ui-standalone-preset.js",
      "dest": "src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ]
}
